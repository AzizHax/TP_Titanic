kable(table_classe_survie, caption = "Survie selon la classe")
ggplot(TitanicSurvival, aes(x = sex, fill = survived)) +
geom_bar(position = "dodge") +
labs(title = "Survie selon le sexe",
x = "Sexe",
y = "Nombre de passagers") +
theme_minimal()
ggplot(TitanicSurvival, aes(x = passengerClass, fill = survived)) +
geom_bar(position = "dodge") +
labs(title = "Survie selon la classe",
x = "Classe",
y = "Nombre de passagers") +
theme_minimal()
ggplot(TitanicSurvival, aes(x = survived, y = age)) +
geom_boxplot() +
labs(title = "Distribution de l'âge selon la survie",
x = "Survie",
y = "Âge") +
theme_minimal()
# Modèle logistique pour expliquer la survie
model <- glm(survived ~ sex + passengerClass + age,
data = TitanicSurvival,
family = binomial)
# Résumé du modèle
summary(model)
